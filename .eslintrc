{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "rules": {
    "global-require": 0,
    "import/no-extraneous-dependencies": 0,
    "import/prefer-default-export": 0,
    "indent": ["error", 2],
    "max-len": 1,
    "no-confusing-arrow": 0,
    "no-console": 0,
    "no-undef": 0,
    "react/jsx-filename-extension": 0,
    "react/no-array-index-key": 1,
    "react/prop-types": 0,
    "react/require-default-props": 0,
    "space-infix-ops": 0
  },
}
