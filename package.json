{
  "name": "lanceguyatt",
  "version": "2.0.5",
  "description": "Personal site built with React Lite, React Router, Webpack, PostCSS, CSS Modules, and Babel.",
  "author": "Lance Guyatt",
  "repository": {
    "type": "git",
    "url": "git@github.com:lanceguyatt/lanceguyatt.git"
  },
  "license": "MIT",
  "keywords": [
    "react",
    "react-router",
    "webpack",
    "postcss",
    "css modules",
    "babel"
  ],
  "scripts": {
    "dev": "babel-node ./tools/dev_server.js",
    "start": "serve build -s -c 1",
    "clean": "rimraf build",
    "prestart": "npm run build",
    "build:bundle": "cross-env NODE_ENV=production webpack --config ./tools/webpack.prod.config.js",
    "build:static": "cross-env NODE_ENV=production babel-node ./tools/build_static_entry.js",
    "build": "npm run clean && npm run build:bundle && npm run build:static",
    "deploy": "npm run build && surge -p build -d lanceguyatt.com",
    "test": "./node_modules/karma/bin/karma start ./test/karma.conf.js",
    "format": "prettier --write \"**/*.js\"",
    "lint": "eslint \"**/*.js\""
  },
  "plate_config": {
    "website": "http://lanceguyatt.com"
  },
  "engines": {
    "node": "~8.11.3",
    "npm": "~5.6.0"
  },
  "dependencies": {
    "history": "^4.7.2",
    "prop-types": "^15.5.10",
    "react": "^16.4.1",
    "react-css-transition": "^0.7.4",
    "react-dom": "^16.4.1",
    "react-fastclick": "^3.0.2-alpha.1",
    "react-ga": "^2.5.3",
    "react-helmet": "^5.1.3",
    "react-lite": "^0.15.37",
    "react-router": "4.3.1",
    "react-router-scroll": "^0.4.2",
    "serve": "^9.1.0"
  },
  "devDependencies": {
    "autoprefixer": "^8.6.3",
    "babel-cli": "^6.24 .0",
    "babel-core": "^6.25.0",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.0.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "compression-webpack-plugin": "^1.1.11",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^5.0.1",
    "css-loader": "^0.28.4",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.0.1",
    "expect": "^23.1.0",
    "express": "^4.15.3",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.11",
    "image-webpack-loader": "^4.3.1",
    "json-loader": "^0.5.4",
    "karma": "^2.0.4",
    "karma-chrome-launcher": "^2.1.1",
    "karma-mocha": "^1.3.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^3.0.0",
    "lodash": "^4.17.4",
    "mkdirp": "^0.5.1",
    "mocha": "^5.2.0",
    "moment": "^2.18.1",
    "postcss-calc": "^6.0.0",
    "postcss-custom-media": "^6.0.0",
    "postcss-custom-properties": "^7.0.0",
    "postcss-custom-selectors": "^4.0.1",
    "postcss-import": "^11.1.0",
    "postcss-loader": "^2.0.5",
    "postcss-nested": "^3.0.0",
    "postcss-short": "^4.1.0",
    "prettier": "^1.13.5",
    "react-addons-test-utils": "^15.5.1",
    "redbox-react": "^1.4.0",
    "rimraf": "^2.6.1",
    "sitemap": "^1.12.0",
    "style-loader": "^0.21.0",
    "stylelint": "^9.3.0",
    "stylelint-config-standard": "^18.2.0",
    "url-loader": "^1.0.1",
    "webpack": "^4.12.0",
    "webpack-dev-middleware": "^3.1.3",
    "webpack-hot-middleware": "^2.18.0",
    "webpack-isomorphic-tools": "^3.0.2"
  }
}
