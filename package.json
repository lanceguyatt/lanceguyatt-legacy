    "redbox-react": "^1.3.3",
    "rimraf": "^2.5.4",
    "sitemap": "^1.8.2",
    "style-loader": "^0.13.1",
    "stylelint": "^7.5.0",
    "stylelint-config-standard": "^16.0.0",
    "url-loader": "^0.5.7",
    "webpack": "^2.2.1",
    "webpack-dev-middleware": "^1.8.4",
    "webpack-hot-middleware": "^2.13.2",
    "webpack-isomorphic-tools": "^2.6.6"
  },
  "author": "Yannik Schweinzer",
  "repository": {
    "type": "git",
    "url": "https://github.com/webyak/react-static-plate"
