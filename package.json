{
  "name": "lanceguyatt",
  "version": "2.0.5",
  "description": "Personal site built with React Lite, React Router, Webpack, PostCSS, CSS Modules, and Babel.",
  "author": "Lance Guyatt",
  "repository": {
    "type": "git",
    "url": "git@github.com:lanceguyatt/lanceguyatt.git"
  },
  "license": "MIT",
  "keywords": [
    "react",
    "react-router",
    "webpack",
    "postcss",
    "css modules",
    "babel"
  ],
  "scripts": {
    "dev": "babel-node ./tools/dev_server.js",
    "start": "serve build -s -c 1",
    "clean": "rimraf build",
    "prestart": "npm run build",
    "build:bundle": "cross-env NODE_ENV=production webpack --config ./tools/webpack.prod.config.js",
    "build:static": "cross-env NODE_ENV=production babel-node ./tools/build_static_entry.js",
    "build": "npm run clean && npm run build:bundle && npm run build:static",
    "deploy": "npm run build && surge -p build -d lanceguyatt.com",
    "test": "./node_modules/karma/bin/karma start ./test/karma.conf.js"
  },
  "plate_config": {
    "website": "http://lanceguyatt.com"
  },
  "engines": {
    "node": ">=4.4"
  },
  "dependencies": {
    "history": "^3.0.0",
    "prop-types": "^15.5.8",
    "react": "^15.5.4",
    "react-css-transition": "^0.7.4",
    "react-dom": "^15.5.4",
    "react-fastclick": "^3.0.1",
    "react-ga": "^2.2.0",
    "react-helmet": "^5.0.2",
    "react-lite": "^0.15.34",
    "react-router": "^3.0.0",
    "react-router-scroll": "^0.4.2",
    "serve": "^5.1.2"
  },
  "devDependencies": {
    "autoprefixer": "^6.7.7",
    "babel-cli": "^6.24 .0",
    "babel-core": "^6.24.0",
    "babel-eslint": "^7.2.1",
    "babel-loader": "^6.4.1",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "compression-webpack-plugin": "^0.4.0",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^4.0.0",
    "css-loader": "^0.28.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "expect": "^1.20.2",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^2.0.0-beta.5",
    "file-loader": "^0.11.1",
    "image-webpack-loader": "^3.3.0",
    "json-loader": "^0.5.4",
    "karma": "^1.6.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-mocha": "^1.3.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.2",
    "lodash": "^4.16.6",
    "mkdirp": "^0.5.1",
    "mocha": "^3.2.0",
    "moment": "^2.18.1",
    "postcss-calc": "^5.3.1",
    "postcss-custom-media": "^5.0.1",
    "postcss-custom-properties": "^5.0.2",
    "postcss-custom-selectors": "^3.0.0",
    "postcss-import": "^9.1.0",
    "postcss-loader": "^1.2.2",
    "postcss-nested": "^1.0.1",
    "postcss-short": "^3.0.3",
    "react-addons-test-utils": "^15.5.1",
    "redbox-react": "^1.3.6",
    "rimraf": "^2.5.4",
    "sitemap": "^1.12.0",
    "style-loader": "^0.16.1",
    "stylelint": "^7.10.1",
    "stylelint-config-standard": "^16.0.0",
    "url-loader": "^0.5.7",
    "webpack": "^2.3.3",
    "webpack-dev-middleware": "^1.8.4",
    "webpack-hot-middleware": "^2.18.0",
    "webpack-isomorphic-tools": "^3.0.2"
  }
}
