{
  "name": "ramiga",
  "version": "0.2.1",
  "description": "Build fast static sites with React & CSS Modules.",
  "license": "MIT",
  "private": true,
  "keywords": [
    "react",
    "static",
    "site",
    "boilerplate",
    "postcss",
    "css modules"
  ],
  "scripts": {
    "start:dev": "babel-node ./tools/dev_server.js",
    "start": "npm run start:dev",
    "clean": "rimraf build",
    "build:bundle": "cross-env NODE_ENV=production webpack --config ./tools/webpack.prod.config.js",
    "build:static": "cross-env NODE_ENV=production babel-node ./tools/build_static_entry.js",
    "build": "npm run clean && npm run build:bundle && npm run build:static"
  },
  "plate_config": {
    "website": "http://lanceguyatt.com"
  },
  "engines": {
    "node": ">=4.4"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "history": "^3.0.0",
    "normalize.css": "^5.0.0",
    "ramda": "^0.23.0",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-fastclick": "^3.0.1",
    "react-helmet": "^4.0.0",
    "react-router": "^3.0.0",
    "react-router-scroll": "^0.4.1"
  },
  "devDependencies": {
    "autoprefixer": "^6.7.5",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.2",
    "babel-eslint": "^7.1.0",
    "babel-loader": "^6.2.7",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "cross-env": "^3.1.3",
    "css-loader": "^0.26.1",
    "eslint": "^3.16.0",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.6.0",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^2.0.0-beta.5",
    "file-loader": "^0.10.0",
    "image-webpack-loader": "^3.2.0",
    "json-loader": "^0.5.4",
    "lodash": "^4.16.6",
    "mkdirp": "^0.5.1",
    "postcss-calc": "^5.3.1",
    "postcss-color-function": "^3.0.0",
    "postcss-custom-media": "^5.0.1",
    "postcss-custom-properties": "^5.0.2",
    "postcss-custom-selectors": "^3.0.0",
    "postcss-import": "^9.1.0",
    "postcss-loader": "^1.2.2",
    "postcss-media-minmax": "^2.1.2",
